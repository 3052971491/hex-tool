{
  "name": "@hex-tool/hooks",
  "version": "0.0.1",
  "description": "hooks utils of hex-tool",
  "type": "module",
  "module": "lib/index.js",
  "main": "lib/index.cjs",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm --out-dir=lib --watch --dts",
    "build": "tsup src/index.ts --format esm,cjs --out-dir=lib --dts --clean"
  },
  "files": [
    "lib",
    "tsconfig.json"
  ],
  "keywords": [],
  "author": "junjieyuan",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.2.3"
  },
  "dependencies": {
    "@hex-tool/shared": "workspace:*",
    "vue": "^3.4.34"
  }
}